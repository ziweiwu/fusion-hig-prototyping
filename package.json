{
  "name": "react-hig-datepicker",
  "description": "A datepick that uses Autodesk HIG design",
  "author": "Ziwei Wu",
  "version": "0.1.1",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@hig/icon": "^0.1.3",
    "@hig/styles": "^0.2.3",
    "@hig/text-field": "^0.4.0",
    "@hig/typography": "0.2.0-alpha.53829c48",
    "classnames": "^2.2.6",
    "delay": "^2.0.0",
    "emotion": "^9.2.4",
    "global": "^4.3.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "prop-types": "^15.6.2",
    "react": "^15.4.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^15.6.2",
    "react-emotion": "^9.2.4",
    "react-lifecycles-compat": "^3.0.4",
    "webpack": "^4.8.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "yarn build-css &&  react-scripts start",
    "build": "yarn build-css &&  react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject --env=jsdom",
    "storybook": "yarn build-css && start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "yarn build-css && storybook-to-ghpages"
  },
  "peerDependencies": {
    "react": " ^15.4.1 || ^16.3.2 "
  },
  "devDependencies": {
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.1",
    "react-test-renderer": "^16.4.0",
    "enzyme-adapter-react-15": "^1.0.6",
    "node-sass-chokidar": "^1.3.0",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addon-viewport": "^3.4.8",
    "@storybook/react": "^3.4.6",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-plugin-emotion": "^9.2.4",
    "babel-core": "~6.26.3",
    "babel-preset-jest": "~23.2.0",
    "enzyme": "^2.5.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-jest-enzyme": "^6.0.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.73.0",
    "jest-enzyme": "^3.8.3",
    "react-scripts": "1.1.4",
    "sinon": "^4.1.2",
    "source-map": "^0.7.3"
  }
}
